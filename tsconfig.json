{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
//    "module": "es5",
//    "module": "es6",
    "module": "commonjs",
    "target": "es5",
//    "target": "es5",
//    "jsx": "react",

//    "target": "es6",
//    "module": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "typeRoots": [
      "../node_modules/@types"
    ],
    "lib": ["es2017", "dom"]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}